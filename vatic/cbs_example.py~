from cbs import *

cb = CrystalBallSession()
cb.setnpts(10000)

cb.addinput(cellref='C8', tag=cb.getcell('B8'), dist=N(cb.getcell('C8'), cb.getcell('D8')/3))
cb.addinput(cellref='C9', tag=cb.getcell('B9'), dist=N(cb.getcell('C9'), cb.getcell('D9')/3))
cb.addinput(cellref='C10', tag=cb.getcell('B10'), dist=N(cb.getcell('C10'), cb.getcell('D10')/3))
cb.addinput(cellref='C11', tag=cb.getcell('B11'), dist=N(cb.getcell('C11'), cb.getcell('D11')/3))
cb.addinput(cellref='C12', tag=cb.getcell('B12'), dist=N(cb.getcell('C12'), cb.getcell('D12')/3))
cb.addinput(cellref='C13', tag=cb.getcell('B13'), dist=N(cb.getcell('C13'), cb.getcell('D13')/3))
cb.addinput(cellref='C14', tag=cb.getcell('B14'), dist=N(cb.getcell('C14'), cb.getcell('D14')/3))
cb.addinput(cellref='C15', tag=cb.getcell('B15'), dist=N(cb.getcell('C15'), cb.getcell('D15')/3))

cb.addoutput(cellref='C24', tag=cb.getcell('B24'))
cb.addoutput(cellref='C25', tag=cb.getcell('B25'))
cb.addoutput(cellref='C26', tag=cb.getcell('B26'))
cb.addoutput(cellref='C30', tag=cb.getcell('B30'), LSL=cb.getcell('C19'), USL=cb.getcell('D19'))
cb.addoutput(cellref='C31', tag=cb.getcell('B31'), LSL=cb.getcell('C20'), USL=cb.getcell('D20'))

#print cb
cb.run_mc()
cb.plot('Seal Comp. %')
cb.plot('Gland Fill %')